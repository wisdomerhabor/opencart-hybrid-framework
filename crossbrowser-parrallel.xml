<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OpenCart Test Suite" parallel="tests" thread-count="5">

	<listeners>
		<listener class-name="utilities.ExtentReportListener.java" />
		<!--		<listener class-name="utilities.ReportNGListener.java"/>
-->
	</listeners>

	<test name="Chrome_Test">

		<parameter name="os" value="Windows" />
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="testCases.TC001_User_Registration" />
		</classes>
	</test>

	<test name="Edge_Test">

		<parameter name="os" value="Windows" />
		<parameter name="browser" value="edge" />

		<classes>
			<class name="testCases.TC001_User_Registration" />
		</classes>
	</test>

</suite>
