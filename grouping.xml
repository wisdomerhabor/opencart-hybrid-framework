<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OpenCart Test Suite">

	<groups>
		<run>
			<include name="Master" />
			<!--<include name="Regression" />-->
			<!--<include name="DataDriven" />-->
			<!--<include name="Sanity" />-->
		</run>
	</groups>

	<listeners>
		<listener class-name="utilities.ExtentReportListener" />
		<listener class-name="utilities.ReportNGListener"/>
	</listeners>

	<test thread-count="5" name="Test">

		<parameter name="os" value="Windows" />
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="testCases.TC001_User_Registration_Test" />
			<class name="testCases.TC002_User_Authentication_Test" />
			<class name="testCases.TC003_User_AuthenticationDDT" />
		</classes>

	</test> <!--
	Test -->
</suite> <!--
OpenSuite -->

